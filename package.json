{
  "name": "aes-256-cbc-encryption",
  "version": "0.0.4",
  "description": "This Node.js module provides robust and secure functions for encrypting and decrypting text using the AES-256-CBC (Advanced Encryption Standard with 256-bit key size in Cipher Block Chaining mode) algorithm.",
  "repository": {
    "type": "git",
    "url": "git://github.com/navsqi/aes-256-cbc-encryption.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "dev": "AES_KEY=bf3c199c2470cb477d907b1e0917c17b AES_IV=5183666c72eec9e4 node dist/index.js",
    "test:coverage": "jest --coverage",
    "release": "standard-version",
    "publish": "rm -rf dist/ && tsc && npm publish"
  },
  "keywords": [
    "aes",
    "encrypt",
    "decrypt",
    "aes-256-cbc-encryption"
  ],
  "author": {
    "name": "Nauval S",
    "email": "nauvalsh@gmail.com",
    "url": "https://github.com/navsqi"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.2.3"
  }
}
